package com.lp;

public class Employee {
	private int id;
	private String name;
	public void work() {
		System.out.println("working");
	}
	public int getId() {
		return id;
	}
	public void setId(int id) {
		this.id = id;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	
	
}
